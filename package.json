{
  "name": "rh-11ty",
  "version": "1.0.0",
  "description": "Rick Hocutt Portfolio Site",
  "main": "index.js",
  "scripts": {
    "sass": "sass --style=expanded scss/app.scss css/styles.css",
    "watch:eleventy": "eleventy --serve",
    "watch:sass": "npm run sass -- --watch",
    "start": "npm-run-all sass --parallel watch:*",
    "build": "npm run sass && eleventy"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RickHocutt/rh-11ty.git"
  },
  "author": "Rick Hocutt",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/RickHocutt/rh-11ty/issues"
  },
  "homepage": "https://github.com/RickHocutt/rh-11ty#readme",
  "dependencies": {
    "@11ty/eleventy": "^0.11.0",
    "sass": "^1.26.10"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}
